<%= form_for @user, url: session_create_path do |f| %>
    <%=f.label :username%>
    <%=f.text_field :username%>
    <br>


    <%=f.label :password%>
    <%=f.password_field :password%>
    <br>

    <%=f.submit "login"%>
    <br>
    Or <%= link_to('Log in with Facebook!', '/auth/facebook') %>

<%end%>